<h1 align="center">Log in</h1> 


<%= form_with(url: login_path, local: true, scope: :session) do |form| %>
  <div class="field">
    <%= form.label :email %>
    <%= form.email_field :email%>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password%>
  </div>

  <div class="actions">
    <%= form.submit "Log In"%>
  </div>
<% end %>